  "private": true,
  "dependencies": {
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.2",
    "highcharts": "^11.4.6",
    "highcharts-react-official": "^3.2.1",
